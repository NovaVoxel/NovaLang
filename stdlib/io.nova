# Nova â†’ Python IO
def print(*args: any) -> None:
    __native__("builtins.print", args)

def input(prompt: str = "") -> str:
    __native__("builtins.input", prompt)

def read(path: str) -> str:
    __native__("io.open", path, "r").read()

def write(path: str, data: str) -> None:
    __native__("io.open", path, "w").write(data)
